      sShipping01 est une chaine = _CREATEFORMVALUE("shipping", gmoShipping, vrai)
      sShipping02 est une chaine = _CREATEFORMVALUE("shipping2", gmoShipping2, vrai)
      gsID = "PAYPAL" + DonneIdentifiant()
      
      // Complète
      sHTML est une chaine = [
      <script>
      document.addEventListener('DOMContentLoaded', function() {
      	var iframe = document.createElement('iframe');
      	var html = '<body topmargin=0 leftmargin=0><form id="form%1" name="form%1" action="https://www.sandbox.paypal.com/cgi-bin/webscr" method="post" target="_parent"> \
