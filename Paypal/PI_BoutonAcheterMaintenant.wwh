     name : SetDevise
     procedure_id : 1530260842895033106
     type_code : 14
     code : |1-
      // Résumé : Définit la devise de paiement
      // Syntaxe :
      //SetDevise (<nDevise>)
      //
      // Paramètres :
      //	nDevise : Devise (constante eCZK, eDKK, eEUR, eGBP, eHUF, ePLN, eSEK, eUSD et eJPY)
      // Valeur de retour :
      // 	Aucune
      
      Procédure publique SetDevise(nDevise)
      
      	selon nDevise
      		CAS eCZK : gsCurrencyCode = "CZK"
      		CAS eDKK : gsCurrencyCode = "DKK"
      		CAS eEUR : gsCurrencyCode = "EUR"
      		CAS eGBP : gsCurrencyCode = "GBP"
      		CAS eHUF : gsCurrencyCode = "HUF"
      		CAS ePLN : gsCurrencyCode = "PLN"
      		CAS eSEK : gsCurrencyCode = "SEK"
      		cas eUSD : gsCurrencyCode = "USD"			
      		cas eJPY : gsCurrencyCode = "JPY"
      	FIN
      
      	_HTML()
       
