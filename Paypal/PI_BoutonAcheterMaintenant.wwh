      sShipping02, 
      HTM_PAYPAL..alias)
      
      
      // Mode test ?
      si pas pPayPal.gbModeTest alors sHTML = remplace(sHTML, "sandbox.", "")
      // Langue
      si gsLanguage <> "FR" alors sHTML = remplace(sHTML, "fr_FR", "en_US")
      
      // Affichage du code HTML
      gsHTML = ChaîneVersUTF8(sHTML)
      HTM_PAYPAL = gsHTML
